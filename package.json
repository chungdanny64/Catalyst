  },
  "homepage": "https://github.com/chungdanny64/Catalyst#readme",
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@types/jest": "^26.0.15",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "electron": "^11.0.0",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "regenerator-runtime": "^0.13.7",
